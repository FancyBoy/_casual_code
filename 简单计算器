#include <stdio.h>
int main()
{
float num1,num2,result;
char op;
while(3==scanf("%g%c%g",&num1,&op,&num2))
{
switch(op)
{
case '+':
result=num1+num2;
break;
case '-':
result=num1-num2;
break;
case '*':
result=num1*num2;
break;
case '/':
result=num1/num2;
break;
default:;
}
printf("=%g",result);
}
return 0;
}